{
  "name": "pokefight",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn dev:server & yarn dev:queue",
    "dev:server": "npx sequelize db:migrate && nodemon src/server.js",
    "dev:queue": "npx nodemon src/queue.js",
    "pretest": "NODE_ENV=test sequelize db:migrate && sequelize db:seed:all",
    "test": "NODE_ENV=test jest --detectOpenHandles --collectCoverage --silent",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bull": "^3.29.1",
    "bull-board": "^2.1.3",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6"
  }
}
